extends layouts/banner-content.pug
include ./pageBanner.pug

block append head
    title The Team - Durham CompSoc
    link(href="/team.css" rel="stylesheet")

block title
    +pageBanner('The Team', 'Meet the students shaping the society')

block content
    .container
        div.row
            h2 The Exec
            div.col-md-1

            div.col-md-2
                h3 President
                p Manages everything and ensures that CompSoc runs smoothly.
            div.col-md-2
                h3 Secretary
                p Takes care of sponsors, contracts, paperwork etc.
            div.col-md-2
                h3 Treasurer
                p Handles the financial matters of CompSoc.
            div.col-md-2
                h3 Web Officer
                p Manages the website, the regular updates and the technical side of the society.
            div.col-md-2
                h3 Social Sec
                p Organizes socials and events.
            div.col-md-1

        div.row
            div.panel-group
                div.panel.panel-default
                    div.panel-heading
                        h3
                            a(data-toggle="collapse" href="#ex78") 2017/18
                    #ex78.panel-collapse.collapse
                        div.panel-body
                            //- Rewrite to be more pugish?
                            .row
                                div.col-md-1
                                div.col-md-2
                                    h3 President
                                    img(src="./img/team/Robbie Jakob-Whitworth.jpg").exec-photo
                                    h3 Robbie Jakob-Whitworth
                                div.col-md-2
                                    h3 Secretary
                                    img(src="./img/team/Vivek Mehta.png").exec-photo
                                    h3 Vivek Mehta
                                div.col-md-2
                                    h3 Treasurer
                                    img(src="./img/team/Tom O'Leary.jpg").exec-photo
                                    h3 Tom O'Leary
                                div.col-md-2
                                    h3 Web Officer
                                    img(src="./img/team/soumya2.png").exec-photo
                                    h3 Soumya Singh
                                div.col-md-2
                                    h3 Social Sec
                                    img(src="./img/team/ryota.png").exec-photo
                                    h3 Ryota Sakai
                                div.col-md-1

            div.panel-group
                div.panel.panel-default
                    div.panel-heading
                        h3
                            a(data-toggle="collapse" href="#ex67") 2016/17
                    #ex67.panel-collapse.collapse
                        div.panel-body
                            div.col-md-3
                                h3 President
                                img(src="./img/team/Alistair Madden.png").exec-photo
                                h3 Alistair Madden
                            div.col-md-3
                                h3 Secretary
                                img(src="./img/team/placeholder.png").exec-photo
                                h3 Mitchel Starkie
                            div.col-md-3
                                h3 Treasurer
                                img(src="./img/team/Sara Chen.png").exec-photo
                                h3 Sara Chen
                            div.col-md-3
                                h3 Web Officer
                                img(src="./img/team/Soumya Singh.jpg").exec-photo
                                h3 Soumya Singh

            div.panel-group
                div.panel.panel-default
                    div.panel-heading
                        h3
                            a(data-toggle="collapse" href="#ex56") 2015/16
                    #ex56.panel-collapse.collapse
                        div.panel-body
                                div.col-md-2
                                    h3 President
                                    img(src="./img/team/Alistair Madden.png").exec-photo
                                    h3 Alistair Madden
                                div.col-md-2
                                    h3 Secretary
                                    img(src="./img/team/placeholder.png").exec-photo
                                    h3 Ersil Jabbar
                                div.col-md-2
                                    h3 Treasurer
                                    img(src="./img/team/placeholder.png").exec-photo
                                    h3 Raffe-ul Islam
                                div.col-md-2
                                    h3 Publicity
                                    img(src="./img/team/placeholder.png").exec-photo
                                    h3 Alan Go
                                div.col-md-2
                                    h3 Web Officer
                                    img(src="./img/team/placeholder.png").exec-photo
                                    h3 Ram Gupta
                                div.col-md-2
                                    h3 Librarian
                                    img(src="./img/team/placeholder.png").exec-photo
